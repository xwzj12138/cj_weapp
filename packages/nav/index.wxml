<scroll-view scroll-x class="bg-white nav" scroll-with-animation scroll-left="{{scrollLeft}}">
  <view class='flex text-center' style="font-size:{{fontSize}}rpx">
    <view class="x-item flex-sub {{index==TabCur?'text-green cur':''}}" wx:for="{{tabList}}" wx:key='nav_key' 
    catchtap='tabSelect' data-index="{{index}}">{{item[key]}}</view>
  </view>
</scroll-view>
